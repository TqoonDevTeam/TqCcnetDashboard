<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
    <sectionGroup name="bundleTransformer">
      <section name="core" type="BundleTransformer.Core.Configuration.CoreSettings, BundleTransformer.Core" />
      <section name="sassAndScss" type="BundleTransformer.SassAndScss.Configuration.SassAndScssSettings, BundleTransformer.SassAndScss" />
    </sectionGroup>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.MvcContextHandler, Spring.Web.Mvc5" />
    </sectionGroup>
  </configSections>
  <spring>
    <context />
  </spring>
  <log4net debug="true">
    <appender name="RollingLogFileAppender404" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs\404\%date{yyyyMMdd}.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] – %message%newline" />
      </layout>
    </appender>
    <appender name="RollingLogFileAppender500" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs\500\%date{yyyyMMdd}.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] – %message%newline" />
      </layout>
    </appender>
    <appender name="RollingLogFileAppenderBIZ" type="log4net.Appender.RollingFileAppender">
      <file type="log4net.Util.PatternString" value="logs\BIZ\%date{yyyyMMdd}.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%property{NDC}] – %message%newline" />
      </layout>
    </appender>
    <logger name="RollingLog404">
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppender404" />
    </logger>
    <logger name="RollingLog500">
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppender500" />
    </logger>
    <root>
      <level value="ALL" />
      <appender-ref ref="RollingLogFileAppenderBIZ" />
    </root>
  </log4net>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
